{
  "manifest_version": 2,
  "name": "My Image Extractor",
  "version": "1.0",
  "description": "Extracts all image URLs from the current page.",
  "permissions": ["activeTab", "storage"],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
}
